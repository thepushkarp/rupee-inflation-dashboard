{
  "name": "rupee-inflation-dashboard",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "check": "bun run format:check && bun run lint && bun run css:types:check && bun run typecheck && bun run test && bun run build",
    "css:types": "tcm -p 'src/**/*.module.css' .",
    "css:types:check": "tcm -p 'src/**/*.module.css' -l .",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "lint": "oxlint --tsconfig tsconfig.json src",
    "preview": "vite preview",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "verify:lock": "git diff --exit-code"
  },
  "dependencies": {
    "apexcharts": "5.6.0",
    "react": "19.2.4",
    "react-apexcharts": "2.0.1",
    "react-dom": "19.2.4",
    "swr": "2.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.19.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.4",
    "jsdom": "28.1.0",
    "oxfmt": "0.35.0",
    "oxlint": "1.50.0",
    "typed-css-modules": "0.9.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "engines": {
    "bun": ">=1.3.10",
    "node": ">=22.12.0"
  },
  "packageManager": "bun@1.3.10"
}
